{
  "entries": [
    "./src/main.ts"
  ],
  "options": {
    "binaryFile": "build/cart.wasm",
    "runtime": "incremental",
    "importMemory": true,
    "initialMemory": 1,
    "maximumMemory": 1,
    "noExportMemory": true,
    "zeroFilledMemory": true,
    "memoryBase": 6560,
    "asc_rtrace": true
  },
  "targets": {
    "release": {
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "noAssert": true
    },
    "debug": {
      "sourceMap": "http://localhost:4444/cart.wasm.map",
      "debug": true
    }
  }
}
